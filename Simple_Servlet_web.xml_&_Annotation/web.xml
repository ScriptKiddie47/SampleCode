<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>ScriptKiddie</display-name>
	<servlet>
		<servlet-name>AnyNameWeWant</servlet-name>
		<servlet-class>com.ScriptKiddieJCodes.XmlServlet</servlet-class>
	</servlet>
	<!-- Lets map the above serlvet to a particular URL -->
	<servlet-mapping>
		<servlet-name>AnyNameWeWant</servlet-name>
		<url-pattern>/ItsMagicServletPath</url-pattern>
	</servlet-mapping>
</web-app>